                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT (format(hv.Fecha,"yyyymmdd") &amp; format(hv.Hora,"hh")) as idTiempo, 1 as idSucursal, 
hc.TipoPago as idMedioPago, COUNT(hv.CodVta) as cantidadVentas, SUM(hv.SubT) as montoTotal, ROUND((SUM(hv.SubT)/COUNT(hv.CodVta)),2) as promedioPorVenta
FROM (Historico_Ventas hv
INNER JOIN Historico_Caja hc ON hv.CodVta=hc.CodVta)
WHERE hv.Prec&gt;0
GROUP BY hv.Fecha, hc.TipoPago, format(hv.Hora,"hh")</property>
